<?php $this->headTitle($this->translate('MYPROJECTS')); ?><ul id="projects"><?php foreach ($this->githubResults as $entry) { ?>   <?php echo '<li>'.$entry['name'].'</li>'; ?><?php } ?>    </ul><?php $activityTitle = 'Last '.$this->githubActivityFeed->count().' GitHub actions: '; ?><ul id="activity"><?php echo $this->escape($activityTitle); ?><?php foreach ($this->githubActivityFeed as $entry) { ?>   <?php echo '<li>('.$this->escape($entry->getDateCreated()->toString('F')).') '.$entry->getTitle().'</li>'; ?><?php } ?></ul>