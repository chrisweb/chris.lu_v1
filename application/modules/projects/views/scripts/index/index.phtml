<?php $this->headTitle($this->translate('MYPROJECTS')); ?><section id="core-box">    <?php if (isset($this->githubResults)) { ?>            <?php //Zend_Debug::dump($this->githubResults); ?>            <h2>            <?php echo $this->translate('MYPROJECTS_GITHUB').': '; ?>        </h2>        <div  id="projects" data-role="collapsible-set" data-theme="c" data-content-theme="d">            <?php foreach ($this->githubResults as $entry) { ?>                <div data-role="collapsible">                    <h3><?php echo $entry['title']; ?></h3>                    <div data-role="none">                        <div class="project_header">                            <a href="<?php echo $this->escape($entry['url']); ?>" title="<?php echo $this->escape($entry['title']); ?>" class="repository_link">                                <?php echo $this->escape($entry['url']); ?>                            </a>                            <br>                            <div class="badge">Forks: <?php echo $this->escape($entry['forksCount']); ?></div>                            <br>                            <div class="badge">Watchers: <?php echo $this->escape($entry['watchersCount']); ?></div>                            <br>                        </div>                        <br>                        <?php echo $entry['readme']; ?>                    </div>                </div>            <?php } ?>        </div>        <br>    <?php } ?>        <?php if (isset($this->githubActivityFeed)) { ?>         <h2>            <?php echo 'Last ' . $this->githubActivityFeed['count'] . ' GitHub actions: '; ?>        </h2>        <ul id="activity" class="list-group">            <?php foreach ($this->githubActivityFeed['entries'] as $entry) { ?>                <li class="list-group-item">                    <span class="badge"><?php echo $this->escape($entry['date']); ?></span>                    <?php echo $this->escape($entry['title']); ?>                </li>            <?php } ?>        </ul>    <?php } ?>    </section>