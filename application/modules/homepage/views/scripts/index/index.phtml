<?php $this->headTitle($this->translate('HOME')); ?><?php foreach ($this->paginator as $article) { ?>	<article role="article">		<header>			<h1>				<a href="<?php echo $this->url(array('id' => $article['_id']), 'articleindexread'); ?>"><?php echo $this->escape($article['title']); ?></a>			</h1>			<?php if (array_key_exists('image', $article) && !empty($article['image'])) { ?>				<?php if (array_key_exists('image_alt', $article) && !empty($article['image_alt'])) { ?>					<?php $altAttribute = 'alt="'.$this->escape($article['image_alt']).'" '; ?>				<?php } else { ?>					<?php $altAttribute = ''; ?>				<?php } ?>				<img src="<?php echo '/upload/images/'.$this->escape($article['image']); ?>" <?php echo $altAttribute; ?>/>			<?php } ?>			<?php echo $article['header']; ?>		</header>				<div class="clearfix"></div>				<?php if (array_key_exists('tags', $article) && count($article['tags']) > 0) { ?>					<?php //Zend_Debug::dump($article['tags']); ?>						<footer>				<div class="tags_title"><?php echo $this->translate('TAGS').':'; ?></div>				<?php foreach($article['tags'] as $tag) { ?>					<?php if (!empty($tag['name'])) { ?>						<a class="btn btn-primary" href="<?php echo $this->url(array('id' => $tag['id']), 'articleindextag'); ?>"><?php echo $this->escape($tag['name']); ?></a>					<?php } ?>				<?php } ?>			</footer>				<?php } ?>			</article><?php } ?><div class="paginator"><?php echo $this->paginationControl($this->paginator, null, null, array('routeName' => $this->routeName)); ?></div><fieldset><div class="jsonObject"><?php echo $this->jsonObject; ?></div><span class="jsonObject"><?php echo $this->escape($this->jsonObject); ?></span><pre class="jsonObject"><?php echo $this->jsonObject; ?></pre><code class="jsonObject"><?php echo $this->jsonObject; ?></code><script class="jsonObject"><?php echo $this->jsonObject; ?></script></fieldset><fieldset><div class="jsonString"><?php echo $this->jsonString; ?></div><?p